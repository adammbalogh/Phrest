{
    "name": "ayanozturk/phprest",
    "description": "PHP Rest Framework.",
    "keywords": ["phprest", "framework", "micro", "rest", "restful", "api"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Adam Balogh",
            "email": "baloghbh@gmail.com",
            "homepage": "http://code-chronicle.blogspot.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/http-kernel": "^2.8",
        "league/route": "^1.2",
        "league/container": "^1.1",
        "league/event": "^2.1",
        "willdurand/negotiation": "^1.3",
        "willdurand/hateoas": "^2.3",
        "stack/builder": "^1.0",
        "monolog/monolog": "^1.12",
        "symfony/console": "^2.8",
        "league/booboo": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.2",
        "mockery/mockery": "^1.2.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "bin": ["bin/phprest"],
    "autoload": {
        "psr-4": {
            "Phprest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phprest\\Stub\\": "stub/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "suggest": {
        "phprest/phprest-service-request-filter": "For filtering Entites.",
        "phprest/phprest-service-validator": "For validating Entities.",
        "phprest/phprest-service-orm": "For managing Relational Databases."
    }
}
